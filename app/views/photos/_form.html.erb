
<%= form_for [@event, photo], html: { multipart: true } do |f| %>
  <div class="form-group">
    <div>
    <%= f.label :photo, class: 'text-muted' %>
    </div>
    <%= f.file_field :photo %>
  </div>

  <div class="form-group">
    <div>
    <%= f.label :discription, class: 'text-muted' %>
    </div>
    <%= f.text_field :discription %>
  </div>

  <div class="actions">
    <%= f.submit 'Загрузить', class: 'btn btn-outline-info' %>
  </div>
<% end %>
